{
    "testMatch": [
        "**/__tests__/**/*.[jt]s?(x)",
        "**/?(*.)+(spec|test).[jt]s?(x)",
        "**/__tests__/**/*.mjs?(x)",
        "**/?(*.)+(spec|test).mjs?(x)"
    ],
    "moduleFileExtensions": [
        "mjs",
        "js"
    ],
    "setupFiles": [
        "<rootDir>/.jest/set-process-env.mjs"
    ]
}